/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
	And(a=a, b=b, out=aAndB);
	Not(in=aAndB, out=notAandB);
	Or(a=a, b=b, out=aOrB);
	And(a=notAandB, b=aOrB, out=out);
}